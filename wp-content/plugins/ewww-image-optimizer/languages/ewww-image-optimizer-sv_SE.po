#: common.php:3689
msgid "Xtreme! (Slowest)"
msgstr "Xtreme! (Långsammast)"

#: common.php:3690
msgid "Intense (Slow)"
msgstr "Intensiv (Långsam)"

#: common.php:3691
msgid "Longest Match (Fast)"
msgstr "Längsta matchning (Snabbt)"

#: common.php:3692
msgid "Huffman Only (Faster)"
